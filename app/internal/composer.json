{
  "name": "olcs/olcs-internal",
  "description": "OLCS Internal Web Site",
  "require": {
    "php": "~8.2.0",
    "ext-redis": "*",
    "dvsa/laminas-config-cloud-parameters": "^1.0",
    "laminas/laminas-authentication": "^2.18",
    "laminas/laminas-cache": "^3.13",
    "laminas/laminas-cache-storage-adapter-redis": "^2.10",
    "laminas/laminas-code": "^4.14",
    "laminas/laminas-escaper": "^2.17",
    "laminas/laminas-eventmanager": "^3.14",
    "laminas/laminas-filter": "^2.41",
    "laminas/laminas-form": "^3.21",
    "laminas/laminas-http": "^2.22",
    "laminas/laminas-i18n": "^2.30",
    "laminas/laminas-inputfilter": "^2.33",
    "laminas/laminas-mvc": "^3.8",
    "laminas/laminas-mvc-i18n": "^1.9",
    "laminas/laminas-mvc-plugin-flashmessenger": "^1.11",
    "laminas/laminas-mvc-plugin-prg": "^1.8",
    "laminas/laminas-navigation": "^2.20",
    "laminas/laminas-servicemanager": "^3.23",
    "laminas/laminas-session": "^2.24",
    "laminas/laminas-stdlib": "^3.20",
    "laminas/laminas-validator": "^2.64",
    "laminas/laminas-view": "^2.42",
    "lm-commons/lmc-rbac-mvc": "^3.5",
    "firebase/php-jwt": "^6.11",
    "olcs/olcs-auth": "^9.0",
    "olcs/olcs-common": "^9.0",
    "olcs/olcs-logging": "^8.0",
    "olcs/olcs-transfer": "^8.1.0",
    "olcs/olcs-utils": "^7.0",
    "saeven/zf3-purifier": "^1.1",
    "doctrine/doctrine-module": "^6.3",
    "psr/container": "^1.1|^2",
    "aws/aws-sdk-php": "^3.356"
  },
  "require-dev": {
    "johnkary/phpunit-speedtrap": "^4.0",
    "laminas/laminas-developer-tools": "*",
    "mockery/mockery": "^1.6",
    "phpunit/phpunit": "^9.6",
    "san/san-session-toolbar": "*",
    "bamarni/composer-bin-plugin": "^1.8"
  },
  "autoload": {
    "psr-4": {
      "Olcs\\": "module/Olcs/src",
      "Admin\\": "module/Admin/src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dvsa\\OlcsTest\\": "./test",
      "OlcsTest\\": "./test/Olcs/src",
      "AdminTest\\": "./test/Admin/src",
      "CommonTest\\": "vendor/olcs/olcs-common/test/Common/src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "bin": "echo 'bin not installed'",
    "post-install-cmd": [
      "@composer bin all install"
    ],
    "test": "phpunit",
    "phpcs": "phpcs",
    "psalm": "psalm",
    "phpstan": "phpstan",
    "all": [
      "@test",
      "@phpcs",
      "@psalm",
      "@phpstan"
    ]
  },
  "extra": {
    "bamarni-bin": {
      "bin-links": true,
      "forward-command": true
    }
  },
  "config": {
    "allow-plugins": {
      "laminas/laminas-dependency-plugin": true,
      "bamarni/composer-bin-plugin": true
    },
    "platform": {
      "ext-redis": "6.2.6"
    }
  }
}
